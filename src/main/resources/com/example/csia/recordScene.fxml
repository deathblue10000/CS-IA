<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.LineChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.csia.RecordSceneController">
   <children>
      <Button fx:id="backToMainButton" layoutX="27.0" layoutY="22.0" mnemonicParsing="false" onAction="#switchToMainScene" text="BACK" />
      <LineChart fx:id="lineChart" layoutX="112.0" layoutY="100.0" title="Completion rate for the last 5 Weeks">
        <xAxis>
          <CategoryAxis label="Weeks" side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
      </LineChart>
   </children>
</AnchorPane>
